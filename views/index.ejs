<%- include("partials/header"); -%> 


    <section class="section my-4">
        <div class="container text-center">
            <div class="title">
                <h1>Image Uploader</h1>
                <p class="subtitle">Upload images to the <strong>Server</strong></p>
            </div>

            <div class="row justify-content-center">
                <div class="col-4">
                    <form action="/upload" enctype="multipart/form-data" method="POST">
                        <div class="row">
                            <div class="col-8">
                                <input type="file" class="form-control" name="images" id="formFile" multiple>
                            </div>
                            <div class="col-2">
                                <input type="submit" class="btn btn-warning" value="Upload Images">
                            </div>
                        </div>
                    </form>
                </div>
            </div>

            <section class="mt-5">
                <div class="row">
                    <div class="col-3">
                        <div class="card shadow" style="width: 18rem;">
                            <img src="..." class="card-img-top" class="img-fluid" alt="...">
                            <div class="card-body">
                                <h5 class="card-title font-pop"></h5>
                                <p class="text-secondary font-size-09">Design</p>
                                <a href="#" class="btn btn-info font-size-09 text-light">Read more</a>
                            </div>
                        </div>
                    </div>
                </div>
            </section>

        </div>
    </section>


<%- include("partials/footer"); -%> 
